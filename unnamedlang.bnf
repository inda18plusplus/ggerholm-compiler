<program> ::= <function> { <function> }
<function> ::= <int> <identifier> "(" ")" "{" (<call> | <print>) ";" <return_stmt> "}"
<call> ::= <identifier> "(" ")"
<print> ::= "print" "(" <expression> ")"
<return_stmt> ::= "return" <exp> ";"
<exp> ::= <unary_op> <exp> | <int> | <exp> ("+" | "-" | "*" | "/") <exp> | "(" exp ")"
<unary_op> ::= "!" | "~" | "-"
<identifier> ::= [a-zA-Z]\w*

